    <!-- Info dialog titles/messages extracted from code -->
    <string name="block_all_traffic">Block All Traffic</string>
    <string name="info_block_all_traffic">Requires Firewall (VPN) to be active. Cuts all internet traffic.</string>
    <string name="info_block_play_store">Completely hides the Google Play Store.</string>
    <string name="info_block_developer_options">Prevents access to Developer Options.</string>
    <string name="info_disable_app_settings_control">Prevents users from clearing data or uninstalling apps.</string>
    <string name="info_disable_apk_install">Blocks the installation of apps from unknown sources.</string>
    <string name="info_android_auto_quirk">Blocks Google app and Maps.</string>
    <string name="info_block_new_apps">Blocks newly installed apps until approved.</string>
    <string name="info_disable_tethering_hotspot">Prevents sharing the device internet connection.</string>
    <string name="info_disable_sms_mms">Blocks sending or receiving text messages.</string>
    <string name="info_enable_firewall_vpn">Enables the per-app firewall.</string>
    <string name="info_enable_private_dns">Encrypts DNS queries and prevents changes.</string>
    <string name="info_disallow_adding_users">Prevents creating new user profiles.</string>
    <string name="info_disable_factory_reset">Prevents factory reset from the settings menu.</string>
    <string name="block_gboard_media_search">Block Gboard Media Search</string>
    <string name="info_block_gboard_media_search">Covers GIF, sticker, and image tabs in Gboard with a blank overlay.</string>
    <string name="info_block_whatsapp_updates_tab">Exits WhatsApp when the Updates tab is accessed.</string>
    <string name="info_block_in_app_ai_chats">Blocks Meta AI in WhatsApp and Gemini in Google Messages.</string>
    <string name="info_whitelist_domains">Requires Firewall (VPN) to be active. Allows network requests only to domains listed in an imported whitelist file. Disables Per-App firewall rules when active.</string>
    <string name="info_block_all_in_app_browsers">Blocks in-app browsers for all apps. Add exceptions or per-app blocks via App Management.</string>
    <string name="block_default_browsers">Block Default Browsers</string>
    <string name="info_block_default_browsers">Hides common browser apps and prevents setting any app as the default browser. Your app will handle web links if it has a browser component.</string>
